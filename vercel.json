{
  "version": 2,
  "buildCommand": "cd frontend && pnpm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
